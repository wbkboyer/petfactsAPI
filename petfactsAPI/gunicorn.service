[Unit]
Description=gunicorn daemon
After=network.target

[Service]
User=ubuntu
Group=www-data
WorkingDirectory=/home/ubuntu/petfacts/petfactsAPI/petfactsAPI
ExecStart=/home/ubuntu/petfacts/flask/bin/gunicorn --access-logfile /home/ubuntu/petfacts/logs/gunicorn-error.log --workers 3 --bind unix:/home/ubuntu/petfacts/petfacts.sock wsgi:application

[Install]
WantedBy=multi-user.target
